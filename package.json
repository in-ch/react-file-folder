{
  "name": "@react-toggle-file-tree/monorepo",
  "private": true,
  "version": "1.0.2",
  "description": "Provides easy file tree implementation with React.",
  "author": "in-ch",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/in-ch/react-file-folder.git"
  },
  "scripts": {
    "build": "preconstruct build",
    "watch": "preconstruct watch",
    "prettier:check": "prettier --check .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "yarn test:jest",
    "test:jest": "jest --coverage",
    "e2e": "concurrently --kill-others --success=first --names 'SERVER,E2E' 'yarn start:test' 'yarn test:cypress'",
    "type-check": "tsc --build packages/react-toggle-file-tree/tsconfig.json",
    "precommit": "yarn run type-check & yarn lint & yarn test",
    "postinstall": "preconstruct dev",
    "version": "changeset && changeset version",
    "release": "yarn test && yarn build && changeset publish",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "sb-deploy": "storybook-to-ghpages"
  },
  "dependencies": {
    "@babel/core": "^7.19.6",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-private-methods": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.12.0",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.18.6",
    "@changesets/cli": "^2.26.1",
    "@emotion/jest": "^11.1.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@preconstruct/cli": "^2.6.2",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/nextjs": "^7.6.16",
    "@storybook/storybook-deployer": "^2.8.16",
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "12.1.4",
    "@testing-library/user-event": "^10.0.0",
    "@types/jest": "^29.5.1",
    "@types/jest-in-case": "^1.0.6",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^4.14.0",
    "concurrently": "^7.5.0",
    "cypress": "^5.0.0",
    "eslint": "^7.18.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.3.1",
    "jest-in-case": "^1.0.2",
    "next": "^14.1.0",
    "prettier": "^2.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-loader": "^9.4.4",
    "typescript": "^4.1.3"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "react",
    "react-component",
    "ui",
    "file",
    "file-tree",
    "tree",
    "toggle",
    "toggle-list"
  ],
  "jest": {
    "modulePathIgnorePatterns": [
      "./node_modules"
    ],
    "testRegex": "src/*(/(__tests?__/)([^_].*/)*?[^_][^/]*?\\.(test|spec)?\\.(ts|js)x?)$",
    "setupFilesAfterEnv": [
      "./test-setup.js"
    ],
    "snapshotSerializers": [
      "@emotion/jest/serializer"
    ],
    "testEnvironment": "jsdom"
  },
  "workspaces": [
    "packages/*",
    "docs",
    "storybook"
  ],
  "preconstruct": {
    "packages": [
      "packages/*"
    ],
    "exports": {
      "importConditionDefaultExport": "default"
    }
  },
  "resolutions": {
    "csstype": "^3.0.2"
  },
  "packageManager": "yarn@1.22.19",
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.16",
    "@storybook/addon-interactions": "^7.6.16",
    "@storybook/addon-links": "^7.6.16",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/blocks": "^7.6.16",
    "@storybook/react": "^7.6.16",
    "@storybook/react-webpack5": "^7.6.16",
    "@storybook/test": "^7.6.16",
    "eslint-plugin-storybook": "^0.6.15",
    "storybook": "^7.6.16"
  }
}
